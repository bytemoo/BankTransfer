local function UMPWCNP_fake_script() -- GrayArea.GrayArea 
	local script = Instance.new('Script', GrayArea)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/bytemoo/Team/main/GrayArea"))()
	end)
end
coroutine.wrap(UMPWCNP_fake_script)()
local function RTGIMAA_fake_script() -- TextButton.quitbutton 
	local script = Instance.new('Script', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(RTGIMAA_fake_script)()
